{
  "Logging": {
    "File": {
      "BasePath": "Logs",
      "FileAccessMode": "KeepOpenAndAutoFlush",
      "FileEncodingName": "utf-8",
      "DateFormat": "yyyyMMdd",
      "CounterFormat": "000",
      "MaxFileSize": 10485760,
      "LogLevel": {
        "LogNativoExemplo": "Debug",
        "Default": "Debug" 
      },
      "IncludeScopes": true,
      "MaxQueueSize": 100,
      "Files": [
        
        {
          "Path": "default-<counter>.log"
        },
        
        {
          "Path": "<date:yyyy>/<date:MM>/other-<counter>.log",
          "MinLevel": {
            "MyApp.SomeClass": "Warning",
            "Default": "Trace" 
          },
          "CounterFormat": "00"
        }
      ]
    }
  },
  "LogLevel": {
    "Default": "Information"
  }
}